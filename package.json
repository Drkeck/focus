  "version": "1.0.0",
  "description": "",
  "main": "./server/server.js",
  "dependencies": {
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "mongoose": "^5.9.29",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "simple-peer": "^9.7.2",
